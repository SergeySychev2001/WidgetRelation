{
    "name": "gs-native-js",
    "version": "1.0.0",
    "description": "Native JS API for global server",
    "scripts": {
        "build": "webpack --config ./webpackProd.config.js --mode=production",
        "dev": "webpack serve --config ./webpackDev.config.js --open --mode=development",
        "lint": "eslint src --ext ts,tsx",
        "test": "jest",
        "all-test": "npm run lint && npm run test",
        "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.12",
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.57.1",
        "css-loader": "^7.1.2",
        "eslint": "8.43.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-prettier": "^5.2.1",
        "html-webpack-plugin": "5.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "npm-run-all": "^4.1.5",
        "prettier": "3.3.3",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "9.5.1",
        "typescript": "5.5.4",
        "webpack": "5.94.0",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.2"
    }
}
